{
    "uid": "slack-pencilblue",
    "name": "Slack Pencilblue",
    "description": "Slack integration for Pencilblue",
    "version": "0.1.0",
    "pb_version": ">=0.7.x <1.x",
    "author": {
        "name": "Christopher Satchell",
        "email": "satchell028@gmail.com",
        "website": "https://github.com/dukeofsussex"
    },
    "permissions": {},
    "settings": [
        {
            "displayName": "SLACK_PENCILBLUE_ENABLED",
            "group": "SLACK_PENCILBLUE_GENERAL_SETTINGS",
